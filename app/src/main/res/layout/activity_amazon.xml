<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Spinner
        android:id="@+id/amazon_spinner"
        android:layout_width="150dp"
        android:layout_height="30dp"
        android:entries="@array/amazon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/amazon_outView" />

    <Button
        android:id="@+id/amazon_2"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:text="2"
        android:textSize="25sp"
        app:backgroundTint="#434343"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.352"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.837" />

    <TextView
        android:id="@+id/amazon_souryouView"
        android:layout_width="170dp"
        android:layout_height="70dp"
        android:text="送料"
        app:layout_constraintTop_toBottomOf="@+id/amazon_kakakuView" />

    <Button
        android:id="@+id/amazon_7"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:text="7"
        android:textSize="25sp"
        app:backgroundTint="#434343"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.556" />

    <TextView
        android:id="@+id/amazon_outView"
        android:layout_width="170dp"
        android:layout_height="70dp"
        android:text="結果"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.13" />

    <Button
        android:id="@+id/amazon_period"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:text="."
        android:textSize="25sp"
        app:backgroundTint="#434343"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.632"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.978" />

    <Button
